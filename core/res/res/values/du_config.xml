    <integer name="config_flashlightProximityTimeout">300</integer>

    <!-- Advanced reboot menu -->
    <string-array translatable="false" name="config_custom_globalActionsList">
        <item>power</item>
        <item>restart</item>
        <item>advanced</item>
        <item>screenshot</item>
    </string-array>

</resources>
